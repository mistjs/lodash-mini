{
  "name": "tiny-tool",
  "version": "0.0.1-beta.1",
  "packageManager": "pnpm@7.4.0",
  "description": "Tiny Tool is a tool library that helps us develop quickly",
  "keywords": [
    "lodash",
    "lodash-mini"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT",
  "author": "aibayanyu",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "release": "bumpp",
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:u": "vitest -u"
  },
  "devDependencies": {
    "@mistjs/eslint-config": "^0.0.2",
    "@types/node": "^18.0.0",
    "bumpp": "^8.2.1",
    "conventional-changelog-cli": "^2.2.2",
    "eslint": "^8.18.0",
    "tsup": "^6.1.2",
    "typescript": "^4.7.4",
    "vitest": "^0.16.0"
  }
}
